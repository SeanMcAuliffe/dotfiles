command! W w
command! Q q
command! Wq wq
command! WQ wq
inoremap kj <Esc>
set nowrap
set number
set ruler
set expandtab
set shiftwidth=4
set smartindent
set smarttab
set softtabstop=4
syntax enable
set background=dark
colorscheme habamax
set laststatus=2
set mouse=a
set encoding=utf-8
set colorcolumn=80
